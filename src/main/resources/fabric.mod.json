{
	"schemaVersion": 1,
	"id": "mccourse",
	"version": "${version}",
	"name": "MCCourse Mod",
	"description": "This mod adds a new armor set tools weapons and a new dimension with the new gem the pink garnet. Mostly based on kaupenjoe 1.20.1 Udemy course. I added and changed some things about the mod but it is mostly kapenjoes code Udemy link:https://www.udemy.com/course/minecraft-modding-120x-for-fabric/learn/lecture/39134856#content",


	"authors": [
		"Christopher"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.christopher.mccourse.MCCourseMod"
		],
		"fabric-datagen": [
			"net.christopher.mccourse.MCCourseModDataGenerator"
		],
		"client": [
			"net.christopher.mccourse.MCCourseModClient"
		],
		"rei_client": [
			"net.christopher.mccourse.compat.MCCourseModREIClientPlugin"
		],
		"terrablender": [
			"net.christopher.mccourse.world.biome.ModTerraBlenderAPI"
		]
	},
	"mixins": [
		"mccourse.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.21",
		"minecraft": "~1.20",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}