{
	"schemaVersion": 1,
	"id": "mccourse",
	"version": "${version}",
	"name": "MC Course Mod",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Christopher"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/mccourse/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.christopher.mccourse.MCCourseMod"
		],
		"fabric-datagen": [
			"net.christopher.mccourse.MCCourseModDataGenerator"
		],
		"client": [
			"net.christopher.mccourse.MCCourseModClient"
		],
		"rei_client": [
			"net.christopher.mccourse.compat.MCCourseModREIClientPlugin"
		]
	},
	"mixins": [
		"mccourse.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.14.24",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}